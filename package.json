{
  "name": "bibliomap",
  "version": "3.1.4",
  "description": "BiblioMap shows ezpaarse geolocalized EC's on a map",
  "author": "ezTEAM <ezteam@couperin.org>",
  "scripts": {
    "install": "run-p -ls install:*",
    "install:enricher": "npm --prefix services/enricher install",
    "install:harvester": "npm --prefix services/harvester install",
    "install:viewer": "npm --prefix services/viewer install",
    "lint": "run-p -ls lint:*",
    "lint:admin": "npm --prefix services/admin run lint",
    "lint:enrich": "npm --prefix services/enrich run lint",
    "lint:frontend": "npm --prefix services/frontend run lint"
  },
  "license": "CeCILL",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0"
  }
}
