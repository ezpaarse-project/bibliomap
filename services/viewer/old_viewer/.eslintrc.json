{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prefer-destructuring": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-undef": [
      "off",
      {
        "L": true
      }
    ]
  }
}